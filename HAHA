number = 0
while number < 1 or number > 10:
    print('Enter a positive whole number')
    number = int(input('>'))
    if number > 10:
        print('Number is too large')
    else:
        if number < 1:
            print('Not positive number')
c = 1
for k in range(0, number - 1):
    print(c)
    c = (c * (number -  1 - k)) / (k + 1)

